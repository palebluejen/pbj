// Imports -----------------------------------------------------------------------

@import "compass/reset";
@import "compass/css3/font-face";
@import "compass/css3/background-size";
@import "compass/css3/images";
@import "compass/css3/transform";
@import "compass/css3/transition";
@import "compass/css3/box-shadow";
@import "pbj-home";
@import "base";


// Global -----------------------------------------------------------------------

p {
  margin: 1em 0;
}

em {
  font-family: Georgia, sans-serif;
  font-weight: normal;
  font-style: italic;
  font-size: 80%;
  line-height: 1.4em;
}

span.amp {
  font-family: 'Open Sans', sans-serif;
  font-style: normal;
  font-weight: normal;
  }


// Links -----------------------------------------------------------------------

a {
  color: $link-text-color;
  text-decoration: none;
  padding: 0 0.2em;
  border-bottom: dotted 1px $link-text-color;

  &:hover, a:focus {
    color: white;
    background: $link-text-color;
    border-radius: 0.2em;
  }
}

// Layout -----------------------------------------------------------------------

body {
  font: 1em/1.6em 'Open Sans', sans-serif;
  color: $main-text-color;
  border-left: 4em solid $frame-color;
  border-right: 4em solid $frame-color;
  background:  image-url('backup-bg.jpg') bottom center repeat-x; // Backup image for IE (doesn't load if gradient is supported, filter is no good because it's not multi-stop)
  background-color: $page-bg-color; // Old browsers
  @include background-image(linear-gradient(top, #000000 0%,#000106 4%,#03030f 7%,#04061b 12%,#06061e 17%,#09071d 20%,#0c0a20 23%,#131126 26%,#161429 29%,#20182d 32%,#22192c 35%,#2c1b2b 38%,#3a2029 41%,#3a232d 44%,#482b2d 47%,#4c302f 50%,#5b3f3c 53%,#674a44 56%,#7a5a4d 59%,#846755 62%,#9f8576 65%,#bbab9b 68%,#c6b599 71%,#b28f71 74%,#a47959 77%,#804729 80%,#642110 83%,#5d1910 86%,#51150d 89%,#40110b 92%,#04061b 96%,#22234f 100%));  
  @include breakpoint(baby-bear) {
    border-left: none;
    border-right: none; 
  }
}

/* bottom "border" */
body:after {
  content: "";
  position: fixed;
  background: $frame-color;
  left: 0;
  right: 0;
  height: 4em;
  bottom: 0;

  @include breakpoint(baby-bear) {
    height: 0;
  }  
}

.site-wrapper {
  margin: 4em auto;
  // border: 1px solid $frame-color;
  background:  image-url('stars-01.gif') fixed center center repeat-y /*, image-url('space-19.gif') fixed center center no-repeat*/;
  
  @include breakpoint(papa-bear) { 
    @include background-size(cover);
  }

  @include breakpoint(baby-bear) { 
    margin: 1em auto 0;
    border: none;
  }
}

article {
  padding-top: 15%;
  
  @include breakpoint(mama-bear) { 
    padding-top: 30%;
  }
}

/* break */
article:last-child {
  padding-bottom: 100%;
}

h2, h3 {
  width: 50%;
  max-width: 20em;
  margin: 0 auto 2em;
  padding: 1em 2em;
  
  @include breakpoint(mama-bear) {
    width: 60%;
    padding: 0.5em 1em;
  }
}

.title-block{
  font: 9em 'Novecento', 'Open Sans', sans-serif;
  font-weight: bold;
  color: $heading-text-color;
  text-transform: uppercase;
  text-align: center;
  width: 100%;
  position: absolute;
  letter-spacing: 0.3em;
  margin: 1.5em auto;
  @include animate(bounceIn, 1s, 3s);

  @include breakpoint(mama-bear) { 
    font-size: .5em;
    letter-spacing: 0.5em;
    font-size: 5em;
    margin-top: 2.5em;
  }

  @include breakpoint(baby-bear) { 
    font-size: .3em;
    letter-spacing: 0.5em;
    font-size: 3em;
    margin-top: 3em;
  }
}

.super-title {
  white-space: nowrap;
  display: block;
  position: relative;
  padding: 0 0.5em;
  border-bottom: solid 0.08em #FFFFFF;
  margin: auto;
  width: 4em;
  text-shadow: .1em .1em 0 $main-text-color ;
  filter: progid:DXImageTransform.Microsoft.DropShadow(offX=2,offY=2,color=000000);

  &:last-child {
    border: none;
  }

  @include breakpoint(mama-bear) { 
    width: 5em;
  }

  @include breakpoint(baby-bear) { 
    width: 5em;
  }
}


.section-header {
  font: 1.4em/1.5em 'Novecento', 'Open Sans', sans-serif;
  color: $heading-text-color;
  border: solid $heading-text-color .3em;
  background: $heading-bg-color;
  letter-spacing: 0.1em;
  font-weight: bold;
  text-align: center;

  .debold {
    font-weight: normal;
    display: block;
  }

  @include breakpoint(mama-bear) { 
    font-size: 1em;
    line-height: 1.5em;
  }
}

.text-block {
  width: 70%;
  max-width: 45em;
  margin: auto;
  padding: 2em 3em;
  background: $main-bg-color;
  font-size: 1.1em;

  strong {
    font-weight: bold;
  }

  .special {
    background: $special-bg-color;
    font-style: italic;
    color: $special-text-color;
  }

  @include breakpoint(mama-bear) { 
    width: 75%;
    font-size: 0.9em;
    line-height: 1.4em;
    padding: 1em 1.8em;
  }
}

.subhead {
  font-weight: bold;
  font-size: 120%;
  margin-bottom: 1.5em;

  @include breakpoint(mama-bear) { 
    margin-bottom: 1em;
    font-size: 100%;
  }
}

.my-own-ticket {
  font-size: 80%;
  line-height: 1.5em;
  font-weight: bold;
  margin: 1em 7.3%;
  border: solid 1px #CCCCCC;
  padding: 2em;
}

// Header -----------------------------------------------------------------------

header {
  background: $frame-color;
  position: fixed;
  left: 0; 
  right: 0;
  top: 0;
  // padding: 2em 4em 0em;
  height: 6em;
  z-index: 99;

  @include breakpoint(baby-bear) {
    padding: 0;
    height: 0;
  }
}

.logo {
  @include img-svg(pbj-site-logo);
  @include animate(fadeInDown, 1s, 0s);
  width: 100px;
  height: 30px;
  position: absolute;
  top: 2em;
  left: 4em;

  a {
    @include unpink;
    width: 100px;
    height: 30px;
    display: block;
    text-indent: -9999em;   
    z-index: 100;
    background-position: 0 4em;
    

    &:hover {
      @include img-svg(pbj-site-logo-arrows);
      @include single-transition(background-position, 0.5s, ease)
    }
  }

  @include breakpoint(baby-bear) { 
    display: none;
  }
}


.main-nav {
  position: absolute;
  top: 2em;
  right: 4em;
  display: block;
  font: 1em/2em 'Novecento', 'Open Sans', sans-serif;
  @include animate(fadeInDown, 0.5s, 0s);

  @include breakpoint(baby-bear) { 
    top: 0;
    right: 0;
  }

  span {
    display: inline-block;
    
    @include breakpoint(mama-bear) { 
      margin: 0;
      padding: 0;
    }
  }

  span a {
    color: $nav-color;
    font-weight: bold;
    text-decoration: none;
  }
}

.menu-controls a{
  display: none;
}
  

@include breakpoint(baby-bear){  
  .menu-controls a {
      position: fixed;
      display: block;
      @include img-svg('menu-white');
      background-position: center ;
      background-color: $frame-color;
      text-align: center;
      text-decoration: none;
      transition: all 0.3s ease;
      width: 1.5em;
      height: 1.8em;
      margin: 0.3em;
      border: none;
  }

  .main-nav {
    display: none;
    position: fixed;
    top: 0;
    width: 100%;
    padding: 1em 0;
    background: black;
    text-align: center;
    transition: height 0.3s ease;
    
    a {
      display: inline-block;
      color: white;
      font-size: 0.8em;
      margin: 0;
    }
  }

  #main-nav:target {
    display: block;

    .close-menu {
      display: inline-block;
    }
  }
  
  #main-nav:target + .menu-controls {
       display: none; 
  }
}

.roll-link {
  display: inline-block;
  overflow: hidden;
  vertical-align: top;
  @include perspective(600px);
  @include perspective-origin(50% 50%);

  &:hover {
    text-decoration: none;
  }

  a {
    @include unpink;
    display: block;
    position: relative;
    padding: 0 0.5em;
    @include transition(all 400ms ease);
    @include transform-origin(50%, 0%);
    @include transform-style(preserve-3d);
  }

  a:hover{
    background: $main-bg-color;
    @include translate3d(0, 0, -30px);
    @include rotateX(90deg);
  }

  a:after{
    content: attr(title);
    display: block;
    position: absolute;
    left: 0;
    top: 0;
    padding: 0 0.5em;
    color: $main-text-color;
    background: $main-bg-color;
    @include transform-origin(50%, 0%);
    -webkit-transform: translate3d( 0px, 105%, 0px ) rotateX( -90deg );
    -moz-transform: translate3d( 0px, 105%, 0px ) rotateX( -90deg );
    -ms-transform: translate3d( 0px, 105%, 0px ) rotateX( -90deg );
    transform: translate3d( 0px, 105%, 0px ) rotateX( -90deg );
    // tried to use compass mixins for these transforms, but couldn't figure out the right stuff for "create-transform" and the others can't be used together
    // @include translate3d(0px, 105%, 0px);
    // @include rotateX(-90deg);
    // @include create-transform(false, false, false, false, -90deg, false, false, false, 0px, 105%, 0px, false, false, 50%, 0%);
    }
}


// Belong -----------------------------------------------------------------------

.extra-space {
  margin-bottom: 4em;
}

.blend-box{
  width: 80%;
  max-width: 56.3em;
  margin: 0 auto;

  @include breakpoint(mama-bear) { 
    min-height: 10em;
  }

  @include breakpoint(baby-bear) { 
    width: 85%;
  }

  @include breakpoint(tiny-bear) { 
    width: 90%;
  }
}

.blend-1, .blend-2 {
  width: 100%;
  min-height: 18em;
  background-position: top center;
  background-repeat: no-repeat;
  @include background-size(100% auto);
}

.blend-1 {
  @include img-svg('ice-blend-1');
  margin-top: -5.7%;

  @include breakpoint(mama-bear){
  }


}

.blend-2 {
  @include img-svg('drip-blend-1');
  margin-top: 0%;

}


.text-block {
  width: 70%;
  max-width: 45em;
  margin: auto;
  padding: 2em 3em;
  background: $main-bg-color;
  font-size: 1.1em;

  @include breakpoint(mama-bear) { 
    width: 75%;
    font-size: 0.9em;
    line-height: 1.4em;
    padding: 1em 1.8em;
  }
}

// Column Rules -----------------------------------------------------------------------

.column-container {
  width: 80%;
  max-width: 80em;
  margin: auto auto 5em;
  overflow: hidden;

  @media screen and (min-width: 1000px) and (max-width: 1200px) {
    width: 100%;
    font-size: 1em;
  }

  @media screen and (min-width: 800px) and (max-width: 1000px) {
    width: 95%;
    padding: 2.5%;
  }

  @include breakpoint(mama-bear) {
    width: 98%;
    padding: 1em 0;
  }

}


.column {
  float: left;
  width: 27.3333%;
  min-width: 15em;
  margin: 2%;

  @media screen and (min-width: 800px) and (max-width: 1000px) {
    float: none;
    width: 50%;
    margin: 2% auto;
  }

  @include breakpoint(mama-bear) { 
    float: none;
    width: 70%;
    margin: 2% auto;
  }

  @include breakpoint(tiny-bear) {
    width: 90%;
  }

  .text-block {
    width: 80%;
    padding: 10%;
  }

  ul {
    list-style-type: disc;
    margin: 0 0 0 1.2em;
  }
}


// About -----------------------------------------------------------------------

.pbj-about {
  position: relative;
  background: image-url('pbj-about.png') no-repeat;
  @include background-size(100% auto);
  max-width: 50em;
  width: 85%;
  min-height: 25em;
  margin: auto;

  @include breakpoint(baby-bear) { 
    min-height: 20em;
    line-height: 1.4em;
  }

  @include breakpoint(tiny-bear) { 
    width: 92%;
    min-height: 16em;
    font-size: 70%;
  }

  p {
    padding: 8% 32% 0 8%;
    text-align: center;

    @include breakpoint(tiny-bear) { 
      padding-top: 5%;
    }
  }
}


.column-head {
  width: 75%; /* calc fallback for IE 8 */
  width: calc(100% - 2.6em);
  margin: 2em auto;
  padding: 1em 1em;

  @include breakpoint(mama-bear) { 
    width: 65%;
    padding: 1em 0;
  }
}

.column-1, .column-2, .column-3{
  display: block;
  min-height: 190px;
  background: no-repeat;
  @include background-size(100% auto);

  @media screen and (min-width: 400px) and (max-width: 800px) {
    margin-top: -7em;
    min-height: 350px;
  }
}

.column-1 {
  @include img-svg('column-footer-1');
  margin-top: -15%;
}

.column-2 {
  @include img-svg('column-footer-2');
  margin-top: -12%;
}

.column-3 {
  @include img-svg('column-footer-3');
  margin-top: -15%;
}

.icon-strengths, .icon-chops, .icon-quirks {
  display: block;
  height: 33px;
  margin:  0 auto;

}

.icon-strengths {
  @include img-svg('icon-strengths');
  width: 26px;
}

.icon-chops {
  @include img-svg('icon-chops');
  width: 47px;
}

.icon-quirks {
  @include img-svg('icon-quirks');
  width: 37px;
}

html.svg .impossible, html.no-svg .impossible  {
  @include background-size(40%);
  background-position: center;
}

.impossible-1 {
  @include img-svg('impossible-1');
}

.impossible-2 {
  @include img-svg('impossible-2');
}

.impossible-3 {
  @include img-svg('impossible-3');
}

.impossible-4 {
  @include img-svg('impossible-4');
}

.impossible-5 {
  @include img-svg('impossible-5');
}

.impossible-6 {
  @include img-svg('impossible-6');
}

.impossible-7 {
  @include img-svg('impossible-7');
}

.impossible-8 {
  @include img-svg('impossible-8');
}

// Work -----------------------------------------------------------------------

figure {
  float: left;
  max-width: 350px;
  width: 27.3333%;
  min-width: 15em;
  margin: 2%;

  a {
    @include unpink;
    display: block;
    width: 100%;
    margin: 0;
    padding: 0;
    @include perspective(1000); // hack for smoothing out the blur on hover
  }

  img {
    width: calc(100% - 1em);
    border: solid 0.5em $main-bg-color;
    background: $main-bg-color;
    @include transition(box-shadow .2s ease-in);

    &:hover {
      @include scale(1.1);
      @include single-box-shadow(rgba(0, 0, 0, 0.3), 0, 0, 0.5em, 0.5em);
    }
  }

  @media screen and (min-width: 1000px) and (max-width: 1000px) {
    width: 37%;
    margin: 2%;
  }

  @media screen and (min-width: 800px) and (max-width: 1000px) {
    width: 45%;
  }

  @include breakpoint(mama-bear) { 
    float: none;
    width: 70%;
    margin: 2% auto;
  }

  @include breakpoint(tiny-bear) {
    width: 75%;
  }
}


// Footer -----------------------------------------------------------------------

footer {
  position: relative;
  @include img-svg('theater-seat', repeat-x);
  background-position: right bottom;
  min-height: 200px;
  margin: 0;
  padding-top: 25%;

  @include breakpoint(baby-bear) { 
    padding-top: 35%;
  }

  @include breakpoint(tiny-bear) { 
    padding-top: 55%;
  }
}

.mst3k {
  @include img-svg('mst3k');
  position: absolute;
  width: 200px;
  height: 60px;
  bottom: 50px;
  right: 99px;

  @include breakpoint(baby-bear) { 
    right: 20px;
  }
}

.snark-bucket {
  position: absolute;
  bottom: 120px;
  right: 3em;
  max-width: 45em;
  text-align: right;

  @include breakpoint(mama-bear) { 
    width: 100%;
  }

  @include breakpoint(baby-bear) { 
    right: 0.5em;
  }

  @include breakpoint(tiny-bear) { 
    right: 0;
  }
}

.tom-servo, .joel-robinson, .crow-t-robot {
  position: relative;
  display: inline-block;
  border-radius: 8em;
  background: #FFF;
  color: #000;
  font-size: 0.8em;
  line-height: 1.3em;
  text-align: center;
  vertical-align: bottom;
  }

.tom-servo:after, .joel-robinson:after, .crow-t-robot:after {
  content: '';
  position: absolute;
  border-style: solid;
  border-color: #FFF transparent;
  display: block;
  width: 0;
  }

.tom-servo {
  padding: 2em 1.5em;
  max-width: 15em;
  width: 40%;
  width: calc(46% - 3em);
  margin: 5em 0 0 0;
  }

.tom-servo:after {
  bottom:-1em; /* value = - border-top-width - border-bottom-width */
  right:20%; /* controls horizontal position */
  border-width:2.5em 0 0 3em; /* vary these values to change the angle of the vertex */
  }

.joel-robinson {
  margin: 0 0 10em 0;
  padding: 2em 1.5em;
  max-width: 6.5em;
  min-width: 4em;
  width: 25%;
  width: calc(25% - 3em);
  font-weight: bold;
  }

.joel-robinson:after {
  border-width: 10em 15px 0;
  margin-left: -15px;
  bottom: -9em;
  left: 50%;
  }

.crow-t-robot{
  padding: 2em 1em;
  min-width: 3em;
  width: 15%;
  width: calc(15% - 2em);
  margin: 0 0 1.5em 0;
  }

.crow-t-robot:after {
  bottom:-1.5em; /* value = - border-top-width - border-bottom-width */
  right:40%; /* controls horizontal position */
  border-width:2em 2em 0 0; /* vary these values to change the angle of the vertex */
  }

// Core Values Bubbles & "Hello" Social media Triangles -----------------------------------------------------------------------

.bubble-list, .triangle-list {
  width: 80%;
  max-width: 80em;
  margin: 5em auto;
  list-style: none;
  text-align: center;

  @include breakpoint(mama-bear) { 
    width: 90%;
    margin: 3em auto;
  }
}

.triangle-list {
  @include img-svg('polygon-mesh');
  @include background-size(contain);
  background-position: center ;
  padding: 10%;
  max-width: 60em;

  @include breakpoint(mama-bear) {
    max-width: 80%;
  }
}

.bubble, .triangle {
  position: relative;
  min-width: 6em;
  display: inline-block;
  vertical-align: bottom;
  @include perspective(1000); // hack for smoothing out the blurry text on hover of bubbles and triangles

  html.no-borderradius & {
    min-width: 8em;
    vertical-align: middle;
  }
}

.bubble {
  margin: -0.8em;
}

.triangle {
  margin: -1.3em;
}

.bubble:before, .triangle:before{
  content: "";
  display: block;
  padding-top: 100%;  /* this maintains the aspect ratio of the bubble - 1:1*/
}

.bubble-content, .triangle-content {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  margin: 0;
  font: 1em/1.2em 'Novecento', 'Open Sans', sans-serif;
  font-weight: bold;
  color: $main-text-color;
  text-align: center;
  z-index: 0;

  &:hover {
    @include scale(1.2);
  }

  a {
    color: #000;
    border-color: #000;
  }

  a:hover {
    background-color: transparent;
  }
}

.bubble-content {
  padding: 30% 15%;
  border-radius: 100%;
  background: $main-bg-color;
  @include transition(box-shadow .1s ease-in);

  html.no-borderradius & {
    /* thought about doing a re-sizable svg circle graphic as the bg here, but browsers without border-radius also do not have svg, instead squares */
    border: solid 0.5em black;
    padding: 10%;
  }
  
  em {
    display: block;
    margin: 0.6em 0;
    font-size: 0.9em;
  }

    &:hover {
    @include single-box-shadow(rgba(255, 255, 255, 0.3), 0, 0, 0em, 1em);
  }
}

.triangle-content:before{
  content: "";
  position: absolute;
  width: 100%;
  height: 99%;
  top: 0;
  left: 0;
  @include img-svg('triangle-down');
  @include background-size(100% auto);
  z-index: -1;
}

.triangle:nth-child(even) .triangle-content{
  padding: 30% 15%;
}

.triangle:nth-child(odd) .triangle-content{
  padding: 50% 15%;
  line-height: 3em;
}

.triangle:nth-child(odd) .triangle-content:before{
  @include rotate(180deg);
}

$string-length: 1;

@while $string-length < 100 {
  .size-#{$string-length} {
    width: sqrt($string-length) * 1.75em;
    @include breakpoint(mama-bear) { 
      font-size: 75%;
    }
    @include breakpoint(tiny-bear) { 
      font-size: 60%;
      width: sqrt($string-length) * 1.5em;
    }
  }
  $string-length: $string-length + 1;
}

