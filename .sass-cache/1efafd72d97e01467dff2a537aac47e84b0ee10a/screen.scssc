// Imports -----------------------------------------------------------------------

@import "compass/reset";
@import "compass/css3/font-face";
@import "compass/css3/background-size";
@import "compass/css3/border-radius";
@import "compass/css3/images";
@import "compass/css3/transform";
@import "compass/css3/transition";
// @import "animate";
@import "pbj-home";
@import "base";


// Global -----------------------------------------------------------------------

html, body
{
  font: 1em/1.6em 'Open Sans', sans-serif;
  color: $main-text-color;
  position: relative; /* this seemed to fix the bug where chrome wouldn't show text after making an element absolutely positioned */
}

p {
  margin: 1em 0;
}

em {
  font-family: Georgia, sans-serif;
  font-weight: normal;
  font-style: italic;
}

span.amp {
  font-family: 'Open Sans', sans-serif;
  font-style: normal;
  font-weight: normal;
  }


// Links -----------------------------------------------------------------------

a {
    color: $link-color;
}
a:hover, a:focus {
    color: #2445ae;
    text-decoration: none;
}
a:active {
    color: black;
}

.roll-link {
  display: inline-block;
  overflow: hidden;
  vertical-align: top;
  @include perspective(600px);
  @include perspective-origin(50% 50%);
}

.roll-link:hover {
  text-decoration: none;
}

.roll-link a {
  display: block;
  position: relative;
  padding: 0 0.5em;
  @include transition(all 400ms ease);
  @include transform-origin(50%, 0%);
  @include transform-style(preserve-3d);
}

.roll-link:hover a {
  background: $main-bg-color;
  @include translate3d(0, 0, -30px);
  @include rotateX(90deg);
}

.roll-link a:after {
  content: attr(data-title);
  display: block;
  position: absolute;
  left: 0;
  top: 0;
  padding: 0 0.5em;
  color: $main-text-color;
  background: $main-bg-color;
  @include transform-origin(50%, 0%);
  // @include translate3d(0px, 105%, 0px);
  // @include rotateX(-90deg);

  // @include create-transform(false, false, false, false, -90deg, false, false, false, 0px, 105%, 0px, false, false, 50%, 0%);
  -webkit-transform: translate3d( 0px, 105%, 0px ) rotateX( -90deg );
  -moz-transform: translate3d( 0px, 105%, 0px ) rotateX( -90deg );
  -ms-transform: translate3d( 0px, 105%, 0px ) rotateX( -90deg );
  transform: translate3d( 0px, 105%, 0px ) rotateX( -90deg );
}

// Layout -----------------------------------------------------------------------

body {
  border-left: 4em solid $frame-color;
  border-right: 4em solid $frame-color;  
  background-color: $page-bg-color; // Old browsers
  @include filter-gradient(#000000, #000000, vertical); // IE6-9
  @include background-image(linear-gradient(top, #000000 0%,#000106 4%,#03030f 7%,#04061b 12%,#06061e 17%,#09071d 20%,#0c0a20 23%,#131126 26%,#161429 29%,#20182d 32%,#22192c 35%,#2c1b2b 38%,#3a2029 41%,#3a232d 44%,#482b2d 47%,#4c302f 50%,#5b3f3c 53%,#674a44 56%,#7a5a4d 59%,#846755 62%,#9f8576 65%,#bbab9b 68%,#c6b599 71%,#b28f71 74%,#a47959 77%,#804729 80%,#642110 83%,#5d1910 86%,#51150d 89%,#40110b 92%,#2e0d08 95%,#180604 98%,#000000 100%));

  @include breakpoint(baby-bear) {
    border-left: none;
    border-right: none; 
  }
}

/* bottom "border" */
body:after {
  content: "";
  position: fixed;
  background: $frame-color;
  left: 0;
  right: 0;
  height: 4em;
  bottom: 0;

  @include breakpoint(baby-bear) {
    height: 0;
  }  
}

.site-wrapper {
  margin: 4em auto 3em;
  border: 1px solid $frame-color;
  background:  image-url('stars-01.gif') fixed center center repeat-y /*, image-url('space-19.gif') fixed center center no-repeat*/;
  // background-size: auto;
    @include breakpoint(papa-bear) { 
      @include background-size(cover);
    }
}

article {
  padding-top: 15%;
  // width: 100%;
  
  @include breakpoint(mama-bear) { 
    padding-top: 30%;
  }
  @include breakpoint(baby-bear) { 
    padding-top: 50%;
  }
}

/* break */
article:last-child {
  padding-bottom: 100%;
}

h2, h3 {
  width: 50%;
  max-width: 20em;
  margin: 0 auto 2em;
  padding: 1em 2em;
  
  @include breakpoint(mama-bear) {
    width: 60%;
    padding: 0.5em 1em;
  }
}

.title-block, .pbj-title-block {
  font: 9em 'Novecento', 'Open Sans', sans-serif;
  font-weight: bold;
  color: $heading-text-color;
  text-transform: uppercase;
  margin: 1.5em auto;
  width: 93%;
  width: calc(100% - 1em);

  @include breakpoint(mama-bear) {
    width: calc(100% - 0.1em); /* weird... don't quite understand why.. maybe just don't totally understand ems */
  }

}

.pbj-title-block {
  position: absolute;
  letter-spacing: 0.3em;
}

.title-block {
  position: relative;
  font-size: 4em;
  letter-spacing: 0.1em;
}

.super-title, .super-title:after{
    white-space: nowrap;
    display: block;
    position: relative;
    padding: 0 0.5em;
    width: 4em;
    border-bottom: solid 0.08em #FFFFFF;
    margin: auto;
    text-align: center;

    @include breakpoint(mama-bear) { 
      font-size: .5em;
      letter-spacing: 0.5em;
      width: 5em;
    }

    @include breakpoint(baby-bear) { 
      font-size: .3em;
      letter-spacing: 0.5em;
      width: 5em;
    }
}

.super-title {
    z-index: 0;
    text-shadow: .1em .1em 0 $main-text-color ;
    filter: progid:DXImageTransform.Microsoft.DropShadow(offX=2,offY=2,color=000000);
}

.super-title:after {
    content: attr(data-shadow-text);
    // color: #ff0000;
    color: $main-text-color ;
    border: none;
    text-shadow: none;
    text-align: center;
    position: absolute;
    left: -0.08em;
    top: -0.08em;
    z-index: -1;
    -webkit-mask-image: image-url('text-mask.png');

    @include breakpoint(mama-bear) { 
      font-size: 1em;
    }

    @include breakpoint(baby-bear) { 
      font-size: 1em;
    }
}

.super-title:last-child {
  border: none;
}


.section-header {
  font: 1.4em/1.5em 'Novecento', 'Open Sans', sans-serif;
  color: $heading-text-color;
  border: solid $heading-text-color .3em;
  background: $heading-bg-color;
  letter-spacing: 0.1em;
  font-weight: bold;
  text-align: center;

    .debold {
      font-weight: normal;
      display: block;
    }

    @include breakpoint(mama-bear) { 
      font-size: 1em;
      line-height: 1.5em;
  }
}

.text-block {
  width: 70%;
  max-width: 45em;
  margin: auto;
  padding: 2em 3em;
  background: $main-bg-color;
  font-size: 1.1em;

  strong {
      font-weight: bold;
    }

  .special {
    background: $special-bg-color;
    font-style: italic;
    color: $special-text-color;
  }

  @include breakpoint(mama-bear) { 
    width: 75%;
    font-size: 0.9em;
    line-height: 1.4em;
    padding: 1em 1.8em;
  }
}

.subhead {
  font-weight: bold;
  font-size: 120%;
  margin-bottom: 1.5em;

  @include breakpoint(mama-bear) { 
    margin-bottom: 1em;
    font-size: 100%;
  }
}

// Header -----------------------------------------------------------------------

header {
  background: $frame-color;
  position: fixed;
  left: 0; 
  right: 0;
  top: 0;
  padding: 1em 4em 1em;
  height: 4em;
  z-index: 99;
  @include breakpoint(baby-bear) {
    padding: 1em;
    height: 2.5em;
  }
}

.logo {

  a {
    html.svg & {
      background-image: image-url('pbj-site-logo.svg');
    }
    
    html.no-svg & {
      background-image: image-url('fallbacks/pbj-site-logo.png');
    }

    display: block;
    top: 2em;
    left: 4em;
    position: fixed;
    overflow: hidden;
    background-repeat: no-repeat;
    text-indent: -9999em;   
    width: 100px;
    height: 30px;
    z-index: 100;
    // @include animate(fadeInDown, 1s, 0s);
    
    @include breakpoint(baby-bear) {
      position: absolute;
      width: 60px;
      height: 18px;
      top: 1em;
      left: 1em;
      @include background-size(60px, 18px);
      margin: auto;
      margin-bottom: 1em;
    } 
  }  
}

nav ul.site-nav {
  text-align: right;
  display: block;
  // @include animate(fadeInDown, 1s, 0s);


  // @include breakpoint(baby-bear) { 
  //   text-align: center; 
  // }

    li {
      display: inline-block;
      padding: 0 0 0 1em;
      margin: 1em 0;
      font: 1em/2em 'Novecento', 'Open Sans', sans-serif;
      @include breakpoint(mama-bear) { 
        margin: 0;
        padding: 0;
      }
    }

    li:nth-child(2) {
      padding: 0;
    }

    .belong-nav {
      font: 0.8em/3em 'Open Sans', sans-serif;
      margin-right: 2em;
      @include breakpoint(mama-bear) { 
        width: 100%; 
        padding: 0;
        margin: 0;
        text-align: right;
        line-height: 1.8em;
        font-size: 0.6em;
      }
    }
    
    li a {
      color: $nav-color;
      font-weight: bold;
      text-decoration: none;
      @include breakpoint(baby-bear) { 
        font-size: 0.9em; 
        line-height: 1em;
        margin: 0.4em auto;

      }
    }
}


// Belong -----------------------------------------------------------------------

.string-blend-1, .string-blend-2 {
  display: block;
  margin: 0 auto;
  width: 83%;
  max-width: 56em;
  min-height: 150px;
  background-position: top center;
  background-repeat: no-repeat;
  @include background-size(cover);

  @include breakpoint(mama-bear) { 
    width: 84%;
    @include background-size(cover);
    min-height: 82px;
  }

  @include breakpoint(baby-bear) { 
    width: 88%;
    @include background-size(cover);
    min-height: 60px;
  }
}

.string-blend-1 {
  @include img-svg('string-blend-1');
}

.string-blend-2 {
  @include img-svg('string-blend-2');
}

