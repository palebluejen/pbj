// Work -----------------------------------------------------------------------

figure {
  @extend .column;
  text-align: center;
}


// Core Values Bubbles & "Hello" Social media Triangles -----------------------------------------------------------------------

.bubble-list, .triangle-list {
  width: 80%;
  max-width: 80em;
  margin: 5em auto;
  list-style: none;
  text-align: center;
  @include breakpoint(mama-bear) { 
    width: 90%;
    margin: 3em auto;
  }
}

.triangle-list {
  background: image-url('polygon-mesh-1.gif') top center no-repeat;
  min-height: 40em;
}

.bubble, .triangle {
  position: relative;
  min-width: 6em;
  display: inline-block;
  vertical-align: bottom;

  html.no-borderradius & {
    min-width: 8em;
    vertical-align: middle;
  }
}

.bubble {
  margin: -0.8em;
}

.triangle {
  margin: -1em;
}

.bubble:before, .triangle:before{
  content: "";
  display: block;
  padding-top: 100%;  /* this maintains the aspect ratio of the bubble - 1:1*/
}

.bubble-content {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  padding: 30% 15%;
  margin: 0;
  @include border-radius (100%);
  background: $main-bg-color;
  font: 1em/1.2em 'Novecento', 'Open Sans', sans-serif;
  font-weight: bold;
  color: $main-text-color;
  text-align: center;

  html.no-borderradius & {
    border: solid 0.5em black;
    padding: 10%;
  }
  
  em {
    display: block;
    margin: 0.6em 0;
    font-size: 0.9em;
  }
}

.triangle-content {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  margin: 0;
  z-index: 0;
  // background-image: image-url('triangle-down.svg');
  // @include background-size(100% auto);
  font: 1em/1.2em 'Novecento', 'Open Sans', sans-serif;
  font-weight: bold;
  color: $main-text-color;
  text-align: center;

  html.no-borderradius & {
    border: solid 0.5em black;
    padding: 10%;
  }
  
  em {
    display: block;
    margin: 0.6em 0;
    font-size: 0.9em;
  }
}

// .triangle:nth-child(odd){
//   background: red;
// }

.triangle-content:before{
  content: "";
  position: absolute;
  width: 100%;
  height: 99%;
  top: 0;
  left: 0;
  z-index: -1;
  background-image: image-url('triangle-down.svg');
  @include background-size(100% auto);
  opacity: 50%;
}

.triangle:nth-child(even) .triangle-content{
  padding: 30% 15%;
}

.triangle:nth-child(odd) .triangle-content{
  // top: -50%;
  padding: 50% 15%;
  line-height: 3em;
}

.triangle:nth-child(odd) .triangle-content:before{
  @include rotate(180deg);
}

$string-length: 1;

@while $string-length < 100 {
  .size-#{$string-length} {
    width: sqrt($string-length) * 1.75em;
    // width: percentage(sqrt($string-length) * 0.06);
    @include breakpoint(mama-bear) { 
      font-size: 75%;
    }
  }
  $string-length: $string-length + 1;
}

