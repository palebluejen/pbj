// Imports -----------------------------------------------------------------------

@import "compass/reset";
@import "compass/css3/font-face";
@import "compass/css3/background-size";
@import "compass/css3/border-radius";
@import "compass/css3/images";
@import "compass/css3/transform";
@import "compass/css3/transition";
@import "animate";


// Fonts -----------------------------------------------------------------------

@import url(http://fonts.googleapis.com/css?family=Open+Sans:400italic,400,700);
@include font-face("Novecento",font-files("Novecentowide-Light-webfont.ttf",ttf,"Novecentowide-Light-webfont.woff",woff,"Novecentowide-Light-webfont.otf",opentype),"Novecentowide-Light-webfont.eot",normal,normal);
@include font-face("Novecento",font-files("Novecentowide-Bold-webfont.ttf",ttf,"Novecentowide-Bold-webfont.woff",woff,"Novecentowide-Bold-webfont.otf",opentype),"Novecentowide-Bold-webfont.eot",bold,normal);

// Colors -----------------------------------------------------------------------

$frame-color         : #000000;
$nav-color           : #FFFFFF;
$page-bg-color       : #000000;
$main-text-color     : #000000;
$main-bg-color       : #FFFFFF;
$heading-text-color  : #FFFFFF;
$heading-bg-color    : #000000;
$special-text-color  : #000000;
$special-bg-color    : #FFFAD2;
$link-color          : #0F67FB;

// Global -----------------------------------------------------------------------

html, body
{
  font: 1em/1.6em 'Open Sans', sans-serif;
  color: $main-text-color;
  position: relative; /* this seemed to fix the bug where chrome wouldn't show text after making an element absolutely positioned */
}

p {
  margin: 1em 0;
}

em {
  font-family: Georgia, sans-serif;
  font-weight: normal;
  font-style: italic;
}

span.amp {
  font-family: 'Open Sans', sans-serif;
  font-style: normal;
  font-weight: normal;
  }

// Links -----------------------------------------------------------------------

a {
    color: $link-color;
}
a:hover, a:focus {
    color: #2445ae;
    text-decoration: none;
}
a:active {
    color: black;
}

/* ROLL LINKS */
.roll-link {
  display: inline-block;
  overflow: hidden;
  vertical-align: top;
  @include perspective(600px);
  @include perspective-origin(50% 50%);
}

.roll-link:hover {
  text-decoration: none;
}

.roll-link a {
  display: block;
  position: relative;
  padding: 0 0.5em;
  @include transition(all 400ms ease);
  @include transform-origin(50%, 0%);
  @include transform-style(preserve-3d);
}

.roll-link:hover a {
  // background: $link-color;
  background-color: #08061d; // Old browsers
  @include filter-gradient(#08061d, #40262d, vertical); // IE6-9
  @include background-image(linear-gradient(top, #08061d 0%,#26192b 47%,#40262d 100%));
  @include translate3d(0, 0, -30px);
  @include rotateX(90deg);
  //-webkit-transform: translate3d( 0px, 0px, -30px ) rotateX( 90deg );
  //-moz-transform: translate3d( 0px, 0px, -30px ) rotateX( 90deg );
  //-ms-transform: translate3d( 0px, 0px, -30px ) rotateX( 90deg );
  //transform: translate3d( 0px, 0px, -30px ) rotateX( 90deg );

}

.roll-link a:after {
  content: attr(data-title);
  display: block;
  position: absolute;
  left: 0;
  top: 0;
  padding: 0 0.5em;
  color: $nav-color;
  // background: $link-color;
  background-color: #08061d; // Old browsers
  @include filter-gradient(#08061d, #40262d, vertical); // IE6-9
  @include background-image(linear-gradient(top, #08061d 0%,#26192b 47%,#40262d 100%));
  @include transform-origin(50%, 0%);
  // @include translate3d(0px, 105%, 0px);
  // @include rotateX(-90deg);

  // @include create-transform(false, false, false, false, -90deg, false, false, false, 0px, 105%, 0px, false, false, 50%, 0%);
  -webkit-transform: translate3d( 0px, 105%, 0px ) rotateX( -90deg );
  -moz-transform: translate3d( 0px, 105%, 0px ) rotateX( -90deg );
  -ms-transform: translate3d( 0px, 105%, 0px ) rotateX( -90deg );
  transform: translate3d( 0px, 105%, 0px ) rotateX( -90deg );
}

// Responsive Rules -----------------------------------------------------------------------

@mixin breakpoint($point) {
  @if $point == papa-bear {
    @media (min-width: 1200px) { @content; }
  }
  @else if $point == mama-bear {
    @media (max-width: 800px) { @content; }
  }
  @else if $point == baby-bear {
    @media (max-width: 650px)  { @content; }
  }
}

// Layout -----------------------------------------------------------------------

body {
  border-left: 4em solid $frame-color;
  border-right: 4em solid $frame-color;  
  // image-url('Apollo.jpg') center center no-repeat,  image-url('space-18.gif') fixed center center no-repeat,  
  background-color: $page-bg-color; // Old browsers
  @include filter-gradient(#000000, #000000, vertical); // IE6-9
  @include background-image(linear-gradient(top, #000000 0%,#000106 4%,#03030f 7%,#04061b 12%,#06061e 17%,#09071d 20%,#0c0a20 23%,#131126 26%,#161429 29%,#20182d 32%,#22192c 35%,#2c1b2b 38%,#3a2029 41%,#3a232d 44%,#482b2d 47%,#4c302f 50%,#5b3f3c 53%,#674a44 56%,#7a5a4d 59%,#846755 62%,#9f8576 65%,#bbab9b 68%,#c6b599 71%,#b28f71 74%,#a47959 77%,#804729 80%,#642110 83%,#5d1910 86%,#51150d 89%,#40110b 92%,#2e0d08 95%,#180604 98%,#000000 100%));

  @include breakpoint(baby-bear) {
    border-left: none;
    border-right: none; 
  }
}

/* bottom "border" */
body:after {
  content: "";
  position: fixed;
  background: $frame-color;
  left: 0;
  right: 0;
  height: 4em;
  bottom: 0;

  @include breakpoint(baby-bear) {
    height: 0;
  }  
}

.site-wrapper {
  margin: 4em auto 3em;
  border: 1px solid $frame-color;
  background:  image-url('stars-01.gif') fixed center center repeat-y /*, image-url('space-19.gif') fixed center center no-repeat*/;
  // background-size: auto;
    @include breakpoint(papa-bear) { 
      @include background-size(cover);
    }
}

article {
  padding-top: 15%;
  // width: 100%;
  
  @include breakpoint(mama-bear) { 
    padding-top: 30%;
  }
  @include breakpoint(baby-bear) { 
